//FizzBuzz関数を記載する
//条件分岐による出力結果の分岐
//関数の処理を1行で行えるように簡素化

export function fizzbuzz() {//関数宣言
  let result = [];//出力結果格納用配列
  for (let i = 1; i <= 100; i++) {//処理を100回繰り返す
    if (i % 3 === 0 && i % 5 === 0) {
      result.push("FizzBuzz");
    } else if (i % 3 === 0) {
      result.push("Fizz");
    } else if (i % 5 === 0) {
      result.push("Buzz");
    } else {
      result.push(i.toString());
    }
  }
  return result.join("\n") + "\n";
}
