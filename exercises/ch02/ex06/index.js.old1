//FizzBuzz関数を記載する
//条件分岐による出力結果の分岐

export function fizzbuzz() {
	for( let i = 1; i <= 100; i ++ ) {
	  if( i % 15 === 0 ) {
		  console.log( "FizzBuzz" );
	  } else if ( i % 5 === 0 ) {
		  console.log( "Buzz" );
	  } else if ( i % 3 === 0 ) {
		  console.log( "Fizz" );
	  } else {
		  console.log( i.toString() );
	  }
	}
}
